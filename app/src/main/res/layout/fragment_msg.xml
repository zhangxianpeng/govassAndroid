<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data></data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48"
            android:layout_marginLeft="@dimen/dp_15">

            <TextView
                android:id="@+id/tv_goverment_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/goverment_user"
                android:textColor="#ff3296fa"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_company_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_17"
                android:layout_toRightOf="@+id/tv_goverment_user"
                android:text="@string/company_user"
                android:textColor="#ff303030"
                android:textSize="14sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/iv_add_group"
                android:layout_width="@dimen/dp_15"
                android:layout_height="@dimen/dp_15"
                android:src="@mipmap/ic_increase"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_15"/>
        </RelativeLayout>

        <EditText
            android:id="@+id/et_search"
            style="@style/StyleEditText"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_38"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_10">

            <ImageView
                android:id="@+id/iv_goverment_user"
                android:layout_width="@dimen/dp_38"
                android:layout_height="@dimen/dp_38"
                android:layout_centerVertical="true"
                android:src="@mipmap/address_ic_architecture" />

            <TextView
                android:id="@+id/all_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_toRightOf="@+id/iv_goverment_user"
                android:text="@string/goverment_user"
                android:textColor="#ff303030"
                android:textSize="16sp" />


            <ImageView
                android:id="@+id/iv_manage"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/tv_manage"
                android:src="@mipmap/address_ic_architecture"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_manage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:text="@string/manage"
                android:textColor="#ff5379a0"
                android:textSize="14sp" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_60"
            android:layout_marginTop="@dimen/dp_8"
            android:layout_marginRight="@dimen/dp_10"
            android:background="#F3F3F3" />

        <ExpandableListView
            android:id="@+id/ex_listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_26"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginRight="@dimen/dp_15"
            android:divider="@null"
            android:drawSelectorOnTop="false"
            android:groupIndicator="@null"
            android:overScrollMode="never"
            android:scrollbars="none" />

        <!--<androidx.recyclerview.widget.RecyclerView-->
        <!--android:id="@+id/rv_group_user"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:layout_marginLeft="@dimen/dp_26"-->
        <!--android:layout_marginTop="@dimen/dp_5"-->
        <!--android:layout_marginRight="@dimen/dp_15"-->
        <!--android:overScrollMode="never"-->
        <!--android:scrollbars="none" />-->
    </LinearLayout>
</layout>